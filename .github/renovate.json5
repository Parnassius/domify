{
    lockFileMaintenance: {  // Workaround for https://github.com/renovatebot/renovate/discussions/37753
        lockFileMaintenance: {
            enabled: true,
        },
    },
    extends: [
        'config:recommended',
        'config:semverAllMonthly',
        ':configMigration',
        ':disableRateLimiting',
    ],
    additionalBranchPrefix: '{{datasource}}-',
    commitMessagePrefix: '[{{datasource}}]',
    packageRules: [
        {
            matchDatasources: 'github-releases',
            matchDepTypes: 'uses-with',
            enabled: false,
        },
    ],
    customManagers: [
        {
            customType: 'regex',
            datasourceTemplate: 'github-runners',
            depTypeTemplate: 'github-runner',
            versioningTemplate: 'docker',
            managerFilePatterns: [
                '/^\\.github/workflows/.+\\.ya?ml$/',
            ],
            matchStrings: [
                '["\']?(?<depName>[^\\s]+?)-(?<currentValue>[^\\s]+?)["\']?\\s+# renovate: github-runner',
            ],
        },
    ],
}
